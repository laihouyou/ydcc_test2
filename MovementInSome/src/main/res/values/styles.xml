<resources>
    
    <style name="myPhoneTvType">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">14dip</item>
        <item name="android:ems">4</item>
    </style>
    
        <!-- 二： LinearLayout背景样式：所有属性控件容器 -->
    <style name="myPhoneLayoutBgType">
        <item name="android:gravity">center_vertical|top</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@drawable/white_bg_2</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingBottom">15dip</item>
        <item name="android:paddingLeft">12dip</item>
        <item name="android:paddingRight">12dip</item>
    </style>
    
        <!-- 一： LinearLayout最外层样式：跟父LinearLayout相隔10dip -->
    <style name="myPhoneLayoutPType">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingTop">8dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:paddingLeft">10dip</item>
        <item name="android:paddingRight">10dip</item>
    </style>
    
    <!-- EditText输入框 样式 -->
    <style name="myPhoneEtType">
        <item name="android:background">@drawable/input_bg</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">12dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_height">35dip</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    
    <!-- 定位图片样式 -->
    <style name="myPhoneImageLoc">
        <item name="android:layout_height">25dip</item>
        <item name="android:layout_width">30dip</item>
        <item name="android:background">@drawable/maplocation</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">5dp</item>
    </style>
    
    <!-- 定义Spinner样式 -->
    <style name="myPhoneSpinnerBG">
        <item name="android:layout_height">35dip</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@drawable/spinner_bg</item>
        <item name="android:paddingLeft">3dip</item>
        <item name="android:drawSelectorOnTop">true</item>
    </style>
    
    <!-- 定义单选按钮的样式(wrap_content) -->
    <style name="myPhoneRadioLC">
        <item name="android:drawableLeft">@anim/myradio_button</item>
        <item name="android:layout_height">30dip</item>
        <item name="android:layout_width">30dip</item>
        <item name="android:background">#00000000</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">12dip</item>
        <item name="android:button">@null</item>
    </style>
    
        
    <!-- 四： LinearLayout样式 :每个属性容器 -->
    <style name="myHDLayoutType">
        <item name="android:paddingTop">8dip</item>
        <item name="android:paddingBottom">8dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center_vertical</item>
    </style>    
    
    <!-- 功能图片按钮容器样式 -->
    <style name="myPhoneLinearLayoutFunction">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:gravity">center|top</item>
        <item name="android:orientation">vertical</item>
    </style>
    
    <!-- 功能图片按钮样式 -->
    <style name="myPhoneButtonFunction">
        <item name="android:layout_height">65dip</item>
        <item name="android:layout_width">65dip</item>
    </style>
    
        <!-- 描述字体样式 -->
    <style name="myPhoneTextViewDescribe">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">12dip</item>
    </style>    
    
    <!-- 二： LinearLayout背景样式2：所有属性控件容器带分割标题 -->
    <style name="myPhoneLayoutBgTypeFG">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@drawable/white_bg</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingLeft">12dip</item>
        <item name="android:paddingBottom">10dip</item>
        <item name="android:paddingRight">12dip</item>
    </style>    
    <!-- 背景 -->
    <style name="black_background">
        <item name="android:background">@drawable/bg</item>
    </style>

    <style name="LocationProgressStyle" parent="@android:style/Widget.ProgressBar">
        <item name="android:maxWidth">20.0dip</item>
        <item name="android:maxHeight">20.0dip</item>
        <item name="android:indeterminateOnly">true</item>
        <item name="android:indeterminateDrawable">@drawable/loc_progress_bar</item>
        <item name="android:indeterminateDuration">1200</item>
        <item name="android:indeterminateBehavior">repeat</item>
        <item name="android:minWidth">20.0dip</item>
        <item name="android:minHeight">20.0dip</item>
    </style>

    <!-- 定义大类型Button按钮 -->
    <style name="myPhoneButtonBig">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">30dip</item>
        <item name="android:background">@drawable/btn_next_bg</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#ddffffff</item>
    </style>
    <!-- 定义小类型Button按钮 -->
    <style name="myPhoneButtonSmall">
        <item name="android:layout_width">60dip</item>
        <item name="android:layout_height">30dip</item>
        <item name="android:background">@drawable/btn_icon</item>
        <item name="android:paddingLeft">5dip</item>
        <item name="android:paddingRight">5dip</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <!-- 四： LinearLayout样式 :每个属性容器 -->
    <style name="myPhoneLayoutType">
        <item name="android:paddingTop">3dip</item>
        <item name="android:paddingBottom">5dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <!-- TextView说明样式2 表单内容分类的标题描述 跟myPhoneLayoutBgTypeFG共用 -->
    <style name="myPhoneTvTypeT">
        <item name="android:layout_height">20dip</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">14dip</item>
    </style>

    <!-- 定义单选按钮的样式(match_parent) -->
    <style name="myPhoneRadioRM">
        <item name="android:drawableRight">@anim/myradio_button</item>
        <item name="android:layout_height">40dip</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">#00000000</item>
        <item name="android:button">@null</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">12dip</item>
    </style>

    <style name="myPhoneLayoutTitleType">
        <item name="android:layout_height">45dip</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">#398fdc</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">13dip</item>
        <item name="android:paddingRight">10dip</item>
    </style>

    <style name="myPhoneButtonBack">
        <item name="android:layout_height">20dip</item>
        <item name="android:layout_width">20dip</item>
        <item name="android:background">@drawable/back_icon</item>
    </style>

    <style name="myPhoneTvTitle">
        <item name="android:layout_height">wrap_content</item>
